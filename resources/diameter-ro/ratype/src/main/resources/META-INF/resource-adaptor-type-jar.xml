<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE resource-adaptor-type-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE Resource Adaptor Type 1.1//EN" "http://java.sun.com/dtd/slee-resource-adaptor-type-jar_1_1.dtd">

<resource-adaptor-type-jar>
	<description />

	<resource-adaptor-type>
		<description>Diameter Ro Resource Adaptor Type</description>

		<resource-adaptor-type-name>Diameter Ro</resource-adaptor-type-name>
		<resource-adaptor-type-vendor>java.net</resource-adaptor-type-vendor>
		<resource-adaptor-type-version>0.8.1</resource-adaptor-type-version>

		<resource-adaptor-type-classes>
			<activity-type>
				<activity-type-name>net.java.slee.resource.diameter.ro.RoClientSessionActivity</activity-type-name>
			</activity-type>

			<activity-type>
				<activity-type-name>net.java.slee.resource.diameter.ro.RoServerSessionActivity</activity-type-name>
